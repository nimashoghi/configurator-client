{
    "title": "YouTube2PodBean Settings",
    "type": "object",
    "properties": {
        "PodBean": {
            "type": "object",
            "properties": {
                "ClientId": {
                    "type": "string"
                },
                "ClientSecret": {
                    "type": "string"
                }
            },
            "required": ["ClientId", "ClientSecret"]
        },
        "YouTube": {
            "type": "object",
            "properties": {
                "ChannelId": {
                    "type": "string"
                },
                "PollingRate": {
                    "type": "number"
                },
                "TitlePattern": {
                    "type": "string"
                },
                "TitleNegativePattern": {
                    "type": "string"
                },
                "CustomVideos": {
                    "type": "array",
                    "items": [
                        {
                            "type": "string"
                        }
                    ],
                    "additionalItems": {
                        "title": "Additional item",
                        "type": "number"
                    }
                }
            },
            "required": [
                "ChannelId",
                "PollingRate",
                "TitlePattern",
                "TitleNegativePattern",
                "CustomVideos"
            ]
        },
        "Pickle": {
            "type": "object",
            "properties": {
                "AccessCode": {
                    "type": "string"
                },
                "Processed": {
                    "type": "string"
                },
                "PlaylistHistory": {
                    "type": "string"
                }
            }
        },
        "Server": {
            "type": "object",
            "properties": {
                "Host": {
                    "type": "string"
                },
                "Port": {
                    "type": "string"
                },
                "PublicHost": {
                    "type": "string"
                }
            },
            "required": ["Host", "Port", "PublicHost"]
        }
    },
    "required": ["PodBean", "YouTube", "Server"]
}
